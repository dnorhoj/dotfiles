default partial alphanumeric_keys
xkb_symbols "us-intl-danish" {
    include "us(altgr-intl)"

    // Adding Nordic characters
    key <AD11> { [ bracketleft, braceleft,  aring, Aring  ] }; 
    key <AC10> { [   semicolon,     colon,     ae, AE     ] }; 
    key <AC11> { [  apostrophe,  quotedbl, oslash, Oslash ] }; 

    // Readd acute at tilde, which we just overwrote
    key <TLDE> { [ grave, asciitilde, acute, section ] };
};

